<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>

    </data>
    <androidx.constraintlayout.widget.ConstraintLayout
            android:background="@color/background"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".presention.view.MainActivity">

        <FrameLayout
                android:elevation="12dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content" android:id="@+id/frameFragmentBottom"
                app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent">

        </FrameLayout>
        <ImageView
                android:layout_width="20dp"
                android:layout_height="20dp" app:srcCompat="@drawable/ic_vector_top" android:id="@+id/imageView"
                app:layout_constraintTop_toTopOf="parent" android:layout_marginTop="16dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="16dp"
                android:layout_marginStart="16dp"/>
        <FrameLayout
                android:layout_width="0dp"
                android:layout_height="0dp"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                android:id="@+id/frameFragment"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginTop="12dp" app:layout_constraintTop_toBottomOf="@+id/imageView11">

        </FrameLayout>
        <ImageView
                android:layout_width="24dp"
                android:layout_height="24dp" app:srcCompat="@drawable/ic_iconsmall_top"
                android:id="@+id/imageView11" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
                android:layout_marginRight="8dp" android:layout_marginTop="13dp"
                app:layout_constraintTop_toTopOf="parent"/>
        <TextView
                tools:text="Настройки игы..."
                android:textSize="16sp"
                android:textColor="@color/btnHist1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:id="@+id/textViewTitleTop" android:layout_marginTop="24dp"
                app:layout_constraintTop_toTopOf="parent" app:layout_constraintEnd_toEndOf="parent"
                android:layout_marginEnd="8dp" android:layout_marginRight="8dp"
                app:layout_constraintStart_toStartOf="parent" android:layout_marginLeft="8dp"
                android:layout_marginStart="8dp" android:fontFamily="@font/roboto"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>